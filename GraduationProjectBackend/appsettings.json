{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "OpinionAnalysis": {
    "DateRange": 30,
    "WordCloudTakeNumber": 10,
    "ElasticSearchScorePercentage": 1.0,
    "ElasticSearchArticleTiTleMinimumShouldMatch": "70%",
    "ElasticSearchArticleContentMinimumShouldMatch": "100%",
    "ElasticScrollSize": 100,
    "RedisExpireSecond": 150,
    "RemoveTagWord": [ "水桶", "公告", "新聞" ],
    "WordSegmentWordLength": 0,
    "WordAdjSegmentWordLength": 1,
    "WordNbSegmentWordLength": 1
  }
}
